{
  "title": "Double Shift",
  "rules": [
    {
      "description": "Toggle Caps Lock by pressing Left Shift + Right Shift at the same time",
      "manipulators": [
        {
          "from": {
            "modifiers": {
              "optional": [
                "any"
              ]
            },
            "simultaneous": [
              {
                "key_code": "left_shift"
              },
              {
                "key_code": "right_shift"
              }
            ]
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
