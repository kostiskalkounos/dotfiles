{
  "title": "Right Command",
  "rules": [
    {
      "description": "Change Right Command to f18 if held and return if tapped",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_shift",
                "left_command"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_control",
                "left_command"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": [
                "option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "modifiers": [
                "option"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "option"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "right_command_held",
                "value": 1
              }
            },
            {
              "key_code": "f18"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "right_command_held",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    }
  ]
}
